{
  "name": "grafikart/grafikart.fr",
  "description": "Code source de Grafikart.fr",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.4.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-redis": "*",
    "api-platform/core": "^2.5",
    "aura/sql": "^3.0",
    "beberlei/doctrineextensions": "^1.2",
    "cocur/slugify": "^4.0",
    "doctrine/annotations": "^1.0",
    "erusev/parsedown": "^1.7",
    "everyman/neo4jphp": "dev-master",
    "friendsofphp/php-cs-fixer": "^2.16",
    "geertw/ip-anonymizer": "^1.1",
    "google/apiclient": "^2.0",
    "illuminate/collections": "^8.17",
    "intervention/image": "^2.5",
    "knplabs/knp-paginator-bundle": "^5.4",
    "knpuniversity/oauth2-client-bundle": "^2.3",
    "lcobucci/jwt": "^3.3",
    "league/glide": "^1.6",
    "league/glide-symfony": "^1.0",
    "league/oauth2-facebook": "^2.0",
    "league/oauth2-github": "^2.0",
    "league/oauth2-google": "^3.0",
    "nelmio/cors-bundle": "^1.5 || ^2.0",
    "nucleos/antispam-bundle": "^2.1",
    "omnipay/paypal": "^3.0",
    "oneup/flysystem-bundle": "^3.4",
    "paypal/paypal-checkout-sdk": "^1.0",
    "phpdocumentor/reflection-docblock": "^3.0 || ^4.0 || ^5.0",
    "predis/predis": "^1.1",
    "sensio/framework-extra-bundle": "^5.1",
    "stripe/stripe-php": "^7.31",
    "symfony/asset": "5.2.*",
    "symfony/console": "5.2.*",
    "symfony/doctrine-messenger": "5.2.*",
    "symfony/dotenv": "5.2.*",
    "symfony/expression-language": "5.2.*",
    "symfony/flex": "^1.3.1",
    "symfony/form": "5.2.*",
    "symfony/framework-bundle": "5.2.*",
    "symfony/http-client": "5.2.*",
    "symfony/intl": "5.2.*",
    "symfony/mailer": "5.2.*",
    "symfony/mercure-bundle": "^0.2.3",
    "symfony/messenger": "5.2.*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/notifier": "5.2.*",
    "symfony/orm-pack": "^2.0",
    "symfony/process": "5.2.*",
    "symfony/proxy-manager-bridge": "5.2.*",
    "symfony/redis-messenger": "5.2.*",
    "symfony/security-bundle": "5.2.*",
    "symfony/serializer-pack": "*",
    "symfony/string": "5.2.*",
    "symfony/translation": "5.2.*",
    "symfony/twig-bundle": "5.2.*",
    "symfony/twig-pack": "*",
    "symfony/validator": "5.2.*",
    "symfony/web-link": "5.2.*",
    "symfony/yaml": "5.2.*",
    "twig/cssinliner-extra": "^3.0",
    "twig/extra-bundle": "^3.0",
    "twig/intl-extra": "^3.0",
    "twig/markdown-extra": "^3.0",
    "vich/uploader-bundle": "^1.13",
    "wohali/oauth2-discord-new": "^1.0"
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^6.3",
    "doctrine/doctrine-fixtures-bundle": "^3.3",
    "hautelook/alice-bundle": "^2.8",
    "justinrainbow/json-schema": "^5.2",
    "phpstan/phpstan": "^0.12.54",
    "phpstan/phpstan-doctrine": "^0.12.22",
    "phpstan/phpstan-mockery": "^0.12.3",
    "phpstan/phpstan-phpunit": "^0.12.6",
    "phpstan/phpstan-symfony": "^0.12.11",
    "phpunit/phpunit": "^9.5",
    "spatie/phpunit-watcher": "^1.22",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/debug-pack": "*",
    "symfony/maker-bundle": "^1.0",
    "symfony/profiler-pack": "*",
    "symfony/test-pack": "*",
    "theofidry/alice-data-fixtures": "^1.3"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.2.*"
    }
  }
}
